@startuml

left to right direction

actor "Student"
actor "Car"
actor Database

rectangle "Web Application" {
  usecase "Send Commands to Car" as command
  usecase "View Dashboard" as viewdash
  usecase "Filter Data" as filter
  usecase "Export Data" as export
  usecase "View Instructions" as viewintruc
  usecase "Select Commands" as select
  usecase "Remove Commands" as remove
  usecase "Receive Commands" as receive
  usecase "Display Data" as display
  
  (viewdash) .> (export) : <<extends>>
  (filter) <. (viewdash) : <<extends>>
  (select) .> (command) : <<extends>>
}

Database -- display

Student -- viewdash
Student --viewintruc
Student -- select
Student -- remove

receive -- Car
@enduml
